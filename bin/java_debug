export JAVA_HOME=/usr/local/bin/Java/graalvm-jdk-21.0.1+12.1
export PATH=$JAVA_HOME/bin:$PATH

export PATH=$TORNADO_SDK/bin:$PATH
$TORNADO_SDK/bin/tornado --jvm "\-\-add-modules=jdk.incubator.vector" --jvm "\-Dtornado.logger.codegen=True -agentlib:jdwp=transport=dt_socket,server=y,suspend=y,address=5005" --printKernel --printBytecodes --fullDebug  $@
